class Solution(object):
    def plusOne(self, digits):
        """
        :type digits: List[int]
        :rtype: List[int]
        """
        s = ''
        for integer in digits:
            s = s + str(integer)
        
        result = []
        for char in str(int(s) + 1):
            result.append(char)
        
        return result

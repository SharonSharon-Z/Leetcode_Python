class Solution(object):
    def singleNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        table = {}
        for num in nums:
            if num not in table:
                table[num] = 1
            else:
                table[num] = 2
        
        for item in table:
            if table[item] == 1:
                return item

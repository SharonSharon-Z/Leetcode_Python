class Solution(object):
    def subsets(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        # backtracking
        result= []
        self.helper(nums,result,[],0)
        return result
        
    def helper(self, nums, result, cur, index):
        result.append(cur)
        for i in range(index,len(nums)):
            self.helper(nums,result,cur + [nums[i]],i + 1)

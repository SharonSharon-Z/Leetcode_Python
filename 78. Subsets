class Solution(object):
    def subsets(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        # backtracking
        result= []
        self.helper(nums, 0, [], result)
        return result
        
    def helper(self, nums, index, cur, result):
        print(cur)
        result.append(cur)
        for i in range(index, len(nums)):
            self.helper(nums, i + 1, cur+[nums[i]], result)
